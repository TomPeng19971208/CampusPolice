server {
    listen 80;
    listen [::]:80;

    # TODO: This should be your server name.
    server_name police.zy-peng.com;

    location / {
        proxy_pass https//localhost:4500;
    }

    location /socket {
        proxy_pass https://localhost:4500;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";	 	 
    }
}
